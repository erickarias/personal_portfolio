<div class='project-page content'>
	<div class="inner">
		<h1>{{title}}</h1>
		<button {{action 'showModal' 'detail-modal' model}}>Description</button>
		{{#if hasRelated}}
			<button {{action 'showModal' 'related-modal' model}}>Process</button>
		{{/if}}
		{{#if nextProject}}
			{{#link-to 'project' nextProject classNames='next proj-link'}}<div class="arrow-right"></div>{{/link-to}}
		{{/if}}
		{{#if prevProject}}
			{{#link-to 'project' prevProject classNames='prev proj-link'}}<div class="arrow-left"></div>{{/link-to}}
		{{/if}}
		<div class='browser'>
			<iframe {{bind-attr src='url'}}></iframe>
		</div>
	</div>
</div>