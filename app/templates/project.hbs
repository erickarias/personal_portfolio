<div class='project-page content'>
	<div class="inner">
		<h1>{{title}}</h1>
		<button {{action 'showModal' 'detail-modal' model}}>Details</button>
		{{#if hasRelated}}
			<button {{action 'showModal' 'related-modal' model}}>Wireframes</button>
		{{/if}}
		<div class='browser'>
			<iframe {{bind-attr src='url'}}></iframe>
		</div>
	</div>
</div>